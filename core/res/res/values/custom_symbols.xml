<resources>

    <!-- Package Manager -->
    <java-symbol type="array" name="config_disabledComponents" />
	<java-symbol type="array" name="config_forceEnabledComponents" />

    <java-symbol type="bool" name="config_useSystemClockforRotationSensor" />

    <!-- Usb drive persistent notification -->
    <java-symbol type="bool" name="config_persistUsbDriveNotification" />

    <!-- Extended Power Menu -->
    <java-symbol type="array" name="config_extendedGlobalActionsList" />
    <java-symbol type="drawable" name="ic_restart_quick" />
    <java-symbol type="drawable" name="ic_restart_recovery" />
    <java-symbol type="drawable" name="ic_restart_bootloader" />
    <!-- Defined in res/res/values/symbols.xml -
    <java-symbol type="string" name="global_action_restart" />-->
    <java-symbol type="string" name="global_action_quick_restart" />
    <java-symbol type="string" name="global_action_recovery" />
    <java-symbol type="string" name="global_action_bootloader" />
    <!-- Shutdown Thread -->
    <java-symbol type="string" name="global_restart" />
    <java-symbol type="string" name="global_restart_message" />
    <java-symbol type="string" name="global_quick_restart" />
    <java-symbol type="string" name="global_restart_recovery" />
    <java-symbol type="string" name="global_restart_recovery_message" />
    <java-symbol type="string" name="global_restart_bootloader" />
    <java-symbol type="string" name="global_restart_bootloader_message" />

    <!-- IME selector notification toggle -->
    <java-symbol type="bool" name="config_show_cmIMESwitcher"/>

    <!-- Unlink notification volume -->
    <java-symbol type="drawable" name="ic_audio_notification_new" />
    <java-symbol type="drawable" name="ic_audio_notification_mute_new" />

    <!-- DUI --> 
    <java-symbol type="anim" name="du_app_in" />
    <java-symbol type="anim" name="du_app_out" />

    <!-- Hardware key binding -->
    <java-symbol type="integer" name="config_deviceHardwareKeys" />

    <java-symbol type="integer" name="config_buttonBrightnessSettingDefault" />
    <java-symbol type="bool" name="config_deviceHasVariableButtonBrightness" />

    <!-- Screen recording -->
    <java-symbol type="bool" name="config_enableScreenrecordChord" />

    <!-- Whether device has dash charging support -->
    <java-symbol type="bool" name="config_hasDashCharger" />

    <!-- framework instrumentation and AppProtect features -->
    <java-symbol type="string" name="config_optionalPackageVerifierName" />
    <java-symbol type="integer" name="config_sms_authorization_timeout_ms" />
    <java-symbol type="bool" name="config_sms_authorization_enabled" />

    <!-- Whether notify fingerprint client of successful cancelled authentication -->
    <java-symbol type="bool" name="config_notifyClientOnFingerprintCancelSuccess" />

    <!-- Whether device has turbo power charging support -->
    <java-symbol type="bool" name="config_hasTurboPowerCharger" />

    <!-- The list of vendor package signatures that should also be considered
    platform signatures, specifically for use on devices with a vendor partition. -->
    <java-symbol type="array" name="config_vendorPlatformSignatures" />

    <!-- Path to fast charging status file to detect whether an oem fast charger is active -->
    <java-symbol type="array" name="config_oemFastChargerStatusPaths" />

    <!-- Whether the device requires overlays to be in /system/overlay -->
    <java-symbol type="bool" name="config_hasSystemOverlayPath" />

    <!-- Updater snackbar text color -->
    <java-symbol type="color" name="updater_snackbar_text_color" />

    <!-- Notification info -->
    <java-symbol type="color" name="notification_info_button_text" />

    <!-- Auto fill -->
    <java-symbol type="color" name="autofill_save_title_color" />
    <java-symbol type="color" name="autofill_autofill_save_custom_subtitle_color" />

    <!-- Settings icon tint color -->
    <java-symbol type="color" name="settings_icon_tint_color" />

    <!-- Forcefully tint notification icon text -->
    <java-symbol type="bool" name="config_useDarkBgNotificationIconTextTinting" />
    <java-symbol type="bool" name="config_useDarkBgNotificationTinting_override" />
    <java-symbol type="bool" name="config_notificationTinting_override" />

    <!-- Full screen aspect ratio -->
    <java-symbol type="bool" name="config_haveHigherAspectRatioScreen" />
    <java-symbol type="dimen" name="config_screenAspectRatio" />

    <!-- System fonts -->
    <java-symbol type="array" name="config_fontPackages" />

</resources>
